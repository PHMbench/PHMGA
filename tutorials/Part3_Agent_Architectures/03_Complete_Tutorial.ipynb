    "# ReAct循环概念演示\n",
    "def demonstrate_react_concept():\n",
    "    \"\"\"演示ReAct基本概念\"\"\"\n",
    "    print(\"🔄 ReAct循环演示\")\n",
    "    print(\"=\"*50)\n",
    "    \n",
    "    # 模拟ReAct步骤\n",
    "    problem = \"设备出现异常振动，需要诊断原因\"\n",
    "    print(f\"❓ 问题: {problem}\\n\")\n",
    "    \n",
    "    # Step 1: Thought (思考)\n",
    "    thought1 = \"我需要首先检查传感器数据来了解振动的具体情况\"\n",
    "    print(f\"🤔 Thought 1: {thought1}\")\n",
    "    \n",
    "    # Step 2: Action (行动)\n",
    "    action1 = \"check_sensor_status('vibration')\"\n",
    "    print(f\"🎬 Action 1: {action1}\")\n",
    "    \n",
    "    # Step 3: Observation (观察)\n",
    "    observation1 = \"振动传感器状态: 异常, 当前值: 7.8mm/s (超出正常范围 5.0mm/s)\"\n",
    "    print(f\"👁️ Observation 1: {observation1}\\n\")\n",
    "    \n",
    "    # 继续循环\n",
    "    thought2 = \"振动确实异常，我需要检查可能的原因，比如轴承状态和维护历史\"\n",
    "    print(f\"🤔 Thought 2: {thought2}\")\n",
    "    \n",
    "    action2 = \"get_maintenance_history('bearing001')\"\n",
    "    print(f\"🎬 Action 2: {action2}\")\n",
    "    \n",
    "    observation2 = \"最后维护: 2023-12-10, 类型: 润滑保养, 下次计划: 2024-03-10 (已过期)\"\n",
    "    print(f\"👁️ Observation 2: {observation2}\\n\")\n",
    "    \n",
    "    # 最终推理\n",
    "    final_thought = \"基于振动异常和维护过期的信息，很可能是轴承润滑不足导致的问题\"\n",
    "    print(f\"🎯 Final Thought: {final_thought}\")\n",
    "    \n",
    "    final_answer = \"诊断结果: 轴承润滑不足导致异常振动。建议: 立即进行润滑保养并检查轴承磨损情况。\"\n",
    "    print(f\"📋 Final Answer: {final_answer}\")\n",
    "\n",
    "demonstrate_react_concept()"
   ]
