    "        if \"MaintenancePlanner\" in self.team_agents and results.get(\"diagnosis\"):\n",
    "            plan_request = {**request, \"diagnosis_result\": results[\"diagnosis\"]}\n",
    "            plan_msg = self.send_message(\"MaintenancePlanner\", \"请制定维护计划\", plan_request)\n",
    "            results[\"plan\"] = self.team_agents[\"MaintenancePlanner\"].process_request(plan_request)\n",
    "            print(f\"📨 {self.name} -> MaintenancePlanner: 规划任务已分配\")\n",
    "        \n",
    "        # 3. 生成报告\n",
