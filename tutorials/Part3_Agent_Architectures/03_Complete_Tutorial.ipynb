    "        return {\"status\": \"planning_workflow_completed\"}\n",
    "\n",
    "class DiagnosticAgent(BaseAgent):\n",
    "    \"\"\"诊断Agent - 专门负责设备诊断\"\"\"\n",
    "    \n",
    "    def __init__(self, name: str = \"DiagnosticExpert\", llm_provider: str = \"mock\"):\n",
    "        super().__init__(name, AgentRole.DIAGNOSTICIAN, llm_provider)\n",
    "        self.diagnostic_tools = create_phm_tools()\n",
    "    \n",
    "    def process_request(self, request: Dict[str, Any]) -> Dict[str, Any]:\n",
    "        \"\"\"执行诊断任务\"\"\"\n",
    "        equipment_id = request.get(\"equipment_id\", \"unknown\")\n",
    "        sensor_data = request.get(\"sensor_data\", {})\n",
    "        \n",
    "        print(f\"🔬 {self.name}: 正在诊断设备 {equipment_id}\")\n",
    "        \n",
    "        # 模拟诊断过程\n",
    "        diagnosis = {\n",
    "            \"equipment_id\": equipment_id,\n",
    "            \"status\": \"abnormal\" if any(v > 80 for v in sensor_data.values() if isinstance(v, (int, float))) else \"normal\",\n",
    "            \"issues_found\": [],\n",
    "            \"severity\": \"medium\",\n",
    "            \"confidence\": 0.85\n",
    "        }\n",
    "        \n",
    "        # 检查具体问题\n",
    "        if sensor_data.get(\"temperature\", 0) > 80:\n",
    "            diagnosis[\"issues_found\"].append(\"高温警告\")\n",
    "        if sensor_data.get(\"vibration\", 0) > 5:\n",
    "            diagnosis[\"issues_found\"].append(\"异常振动\")\n",
    "        if sensor_data.get(\"pressure\", 10) < 1.5:\n",
    "            diagnosis[\"issues_found\"].append(\"压力不足\")\n",
    "        \n",
    "        if len(diagnosis[\"issues_found\"]) >= 2:\n",
    "            diagnosis[\"severity\"] = \"high\"\n",
    "        elif len(diagnosis[\"issues_found\"]) == 1:\n",
    "            diagnosis[\"severity\"] = \"medium\"\n",
    "        else:\n",
    "            diagnosis[\"severity\"] = \"low\"\n",
    "        \n",
    "        print(f\"✅ {self.name}: 诊断完成，发现 {len(diagnosis['issues_found'])} 个问题\")\n",
    "        return diagnosis\n",
    "\n",
    "class PlannerAgent(BaseAgent):\n",
    "    \"\"\"规划Agent - 负责制定维护计划\"\"\"\n",
    "    \n",
    "    def __init__(self, name: str = \"MaintenancePlanner\", llm_provider: str = \"mock\"):\n",
    "        super().__init__(name, AgentRole.PLANNER, llm_provider)\n",
    "    \n",
    "    def process_request(self, request: Dict[str, Any]) -> Dict[str, Any]:\n",
    "        \"\"\"制定维护计划\"\"\"\n",
    "        diagnosis = request.get(\"diagnosis_result\", {})\n",
    "        equipment_id = request.get(\"equipment_id\", \"unknown\")\n",
    "        \n",
    "        print(f\"📋 {self.name}: 为设备 {equipment_id} 制定维护计划\")\n",
    "        \n",
    "        issues = diagnosis.get(\"issues_found\", [])\n",
    "        severity = diagnosis.get(\"severity\", \"low\")\n",
    "        \n",
    "        plan = {\n",
    "            \"equipment_id\": equipment_id,\n",
    "            \"immediate_actions\": [],\n",
    "            \"scheduled_maintenance\": [],\n",
    "            \"parts_needed\": [],\n",
    "            \"estimated_cost\": 0,\n",
    "            \"estimated_downtime\": \"0小时\",\n",
    "            \"priority\": severity\n",
    "        }\n",
    "        \n",
    "        # 根据问题生成计划\n",
    "        for issue in issues:\n",
    "            if \"高温\" in issue:\n",
    "                plan[\"immediate_actions\"].append(\"检查冷却系统\")\n",
    "                plan[\"parts_needed\"].append(\"冷却液\")\n",
    "                plan[\"estimated_cost\"] += 500\n",
    "            if \"振动\" in issue:\n",
    "                plan[\"immediate_actions\"].append(\"检查轴承\")\n",
    "                plan[\"parts_needed\"].append(\"轴承润滑脂\")\n",
    "                plan[\"estimated_cost\"] += 800\n",
    "            if \"压力\" in issue:\n",
    "                plan[\"scheduled_maintenance\"].append(\"更换密封件\")\n",
    "                plan[\"parts_needed\"].append(\"密封圈\")\n",
    "                plan[\"estimated_cost\"] += 300\n",
    "        \n",
    "        if severity == \"high\":\n",
    "            plan[\"estimated_downtime\"] = \"6-8小时\"\n",
    "        elif severity == \"medium\":\n",
    "            plan[\"estimated_downtime\"] = \"2-4小时\"\n",
    "        \n",
    "        print(f\"✅ {self.name}: 计划制定完成，预计成本 {plan['estimated_cost']} 元\")\n",
    "        return plan\n",
    "\n",
    "class ReporterAgent(BaseAgent):\n",
    "    \"\"\"报告Agent - 负责生成综合报告\"\"\"\n",
    "    \n",
    "    def __init__(self, name: str = \"ReportGenerator\", llm_provider: str = \"mock\"):\n",
    "        super().__init__(name, AgentRole.REPORTER, llm_provider)\n",
