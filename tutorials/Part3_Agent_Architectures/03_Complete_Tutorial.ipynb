    "        plan = results.get(\"plan\", {})\n",
    "        \n",
    "        report = {\n",
    "            \"report_id\": f\"RPT-{equipment_id}-{int(time.time())}\",\n",
    "            \"equipment_id\": equipment_id,\n",
    "            \"timestamp\": time.strftime(\"%Y-%m-%d %H:%M:%S\"),\n",
    "            \"summary\": {\n",
    "                \"overall_status\": diagnosis.get(\"status\", \"unknown\"),\n",
    "                \"issues_count\": len(diagnosis.get(\"issues_found\", [])),\n",
    "                \"severity_level\": diagnosis.get(\"severity\", \"unknown\"),\n",
    "                \"maintenance_cost\": plan.get(\"estimated_cost\", 0),\n",
    "                \"downtime_estimate\": plan.get(\"estimated_downtime\", \"unknown\")\n",
    "            },\n",
    "            \"detailed_findings\": diagnosis,\n",
    "            \"maintenance_plan\": plan,\n",
    "            \"recommendations\": self._generate_recommendations(diagnosis, plan)\n",
    "        }\n",
    "        \n",
    "        print(f\"✅ {self.name}: 报告生成完成 (ID: {report['report_id']})\")\n",
    "        return report\n",
    "    \n",
    "    def _generate_recommendations(self, diagnosis: Dict, plan: Dict) -> List[str]:\n",
    "        \"\"\"生成建议\"\"\"\n",
    "        recommendations = []\n",
    "        \n",
    "        severity = diagnosis.get(\"severity\", \"low\")\n",
    "        issues = diagnosis.get(\"issues_found\", [])\n",
    "        \n",
    "        if severity == \"high\":\n",
    "            recommendations.append(\"建议立即安排维护，避免设备故障\")\n",
    "        elif severity == \"medium\":\n",
    "            recommendations.append(\"建议在下个维护窗口期间处理\")\n",
    "        \n",
    "        if len(issues) > 2:\n",
    "            recommendations.append(\"多个系统出现问题，建议全面检查\")\n",
    "        \n",
    "        if plan.get(\"estimated_cost\", 0) > 1000:\n",
    "            recommendations.append(\"维护成本较高，建议评估设备更换的经济性\")\n",
    "        \n",
    "        if not recommendations:\n",
    "            recommendations.append(\"设备运行正常，继续定期监控\")\n",
    "        \n",
    "        return recommendations\n",
