    "\n",
    "print(f\"\\n📊 频谱分析测试:\")\n",
    "print(f\"输入: {spectrum_data}\")\n",
    "result = advanced_tools[0].run(spectrum_data)\n",
    "print(f\"输出: {result}\")\n",
    "\n",
    "# 测试寿命预测工具\n",
    "equipment_data = json.dumps({\n",
    "    \"condition_score\": 0.7,\n",
    "    \"operating_hours\": 8000,\n",
    "    \"maintenance_quality\": 0.85\n",
    "})\n",
    "\n",
    "print(f\"\\n⏳ 寿命预测测试:\")\n",
    "print(f\"输入: {equipment_data}\")\n",
    "result = advanced_tools[1].run(equipment_data)\n",
    "print(f\"输出: {result}\")"
