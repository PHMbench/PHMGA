    "# 演示工具设计原则\n",
    "from langchain_core.tools import Tool\n",
    "import json\n",
    "\n",
    "def create_advanced_phm_tools():\n",
    "    \"\"\"创建高级PHM工具集\"\"\"\n",
    "    \n",
    "    def analyze_vibration_spectrum(sensor_data: str) -> str:\n",
    "        \"\"\"分析振动频谱数据\n",
    "        \n",
    "        Args:\n",
    "            sensor_data: JSON格式的传感器数据，包含频率和幅值\n",
    "            \n",
    "        Returns:\n",
    "            频谱分析结果\n",
    "        \"\"\"\n",
    "        try:\n",
    "            data = json.loads(sensor_data)\n",
    "            frequencies = data.get('frequencies', [])\n",
    "            amplitudes = data.get('amplitudes', [])\n",
    "            \n",
    "            if not frequencies or not amplitudes:\n",
    "                return \"错误: 缺少频率或幅值数据\"\n",
    "            \n",
    "            # 模拟频谱分析\n",
    "            max_amp = max(amplitudes)\n",
    "            max_freq = frequencies[amplitudes.index(max_amp)]\n",
    "            \n",
    "            analysis = {\n",
    "                \"dominant_frequency\": f\"{max_freq}Hz\",\n",
    "                \"peak_amplitude\": f\"{max_amp:.2f}mm/s\",\n",
    "                \"analysis\": \"正常\" if max_amp < 5.0 else \"异常\"\n",
    "            }\n",
    "            \n",
    "            return json.dumps(analysis, ensure_ascii=False)\n",
    "            \n",
    "        except json.JSONDecodeError:\n",
    "            return \"错误: 无效的JSON格式\"\n",
    "        except Exception as e:\n",
    "            return f\"分析错误: {str(e)}\"\n",
    "    \n",
    "    def predict_remaining_life(equipment_data: str) -> str:\n",
    "        \"\"\"预测设备剩余寿命\n",
    "        \n",
    "        Args:\n",
    "            equipment_data: JSON格式的设备数据，包含历史状态和当前参数\n",
    "            \n",
    "        Returns:\n",
    "            寿命预测结果\n",
    "        \"\"\"\n",
    "        try:\n",
    "            data = json.loads(equipment_data)\n",
    "            current_condition = data.get('condition_score', 0.8)  # 0-1范围\n",
    "            operating_hours = data.get('operating_hours', 8760)  # 年运行小时数\n",
    "            maintenance_quality = data.get('maintenance_quality', 0.8)  # 维护质量\n",
    "            \n",
    "            # 简单的寿命预测模型\n",
    "            base_life = 50000  # 基础寿命小时数\n",
    "            condition_factor = current_condition\n",
    "            maintenance_factor = maintenance_quality\n",
    "            \n",
    "            estimated_remaining = base_life * condition_factor * maintenance_factor\n",
    "            remaining_years = estimated_remaining / operating_hours\n",
    "            \n",
    "            confidence = min(0.95, 0.5 + condition_factor * maintenance_factor * 0.5)\n",
    "            \n",
    "            result = {\n",
    "                \"remaining_hours\": int(estimated_remaining),\n",
    "                \"remaining_years\": round(remaining_years, 1),\n",
    "                \"confidence\": round(confidence, 2),\n",
    "                \"recommendation\": \"良好\" if remaining_years > 2 else \"需要关注\" if remaining_years > 1 else \"计划更换\"\n",
    "            }\n",
    "            \n",
    "            return json.dumps(result, ensure_ascii=False)\n",
    "            \n",
    "        except Exception as e:\n",
    "            return f\"预测错误: {str(e)}\"\n",
    "    \n",
    "    def generate_maintenance_plan(equipment_status: str) -> str:\n",
    "        \"\"\"生成维护计划\n",
    "        \n",
    "        Args:\n",
    "            equipment_status: JSON格式的设备状态信息\n",
    "            \n",
    "        Returns:\n",
    "            详细的维护计划\n",
    "        \"\"\"\n",
    "        try:\n",
    "            status = json.loads(equipment_status)\n",
    "            issues = status.get('issues', [])\n",
    "            criticality = status.get('criticality', 'medium')\n",
    "            \n",
    "            # 根据问题生成计划\n",
    "            plan = {\n",
    "                \"immediate_actions\": [],\n",
    "                \"weekly_tasks\": [],\n",
    "                \"monthly_tasks\": [],\n",
    "                \"estimated_cost\": 0,\n",
    "                \"estimated_downtime\": \"0小时\"\n",
    "            }\n",
    "            \n",
    "            if '高温' in str(issues):\n",
    "                plan['immediate_actions'].append('检查冷却系统')\n",
    "                plan['weekly_tasks'].append('清理散热器')\n",
    "                plan['estimated_cost'] += 500\n",
    "            \n",
    "            if '振动' in str(issues):\n",
    "                plan['immediate_actions'].append('检查轴承状态')\n",
    "                plan['monthly_tasks'].append('轴承润滑保养')\n",
    "                plan['estimated_cost'] += 1200\n",
    "            \n",
    "            if criticality == 'high':\n",
    "                plan['estimated_downtime'] = '4-8小时'\n",
    "            elif criticality == 'medium':\n",
    "                plan['estimated_downtime'] = '2-4小时'\n",
    "            \n",
    "            return json.dumps(plan, ensure_ascii=False, indent=2)\n",
    "            \n",
    "        except Exception as e:\n",
    "            return f\"计划生成错误: {str(e)}\"\n",
    "    \n",
    "    # 创建工具列表\n",
    "    tools = [\n",
    "        Tool(\n",
    "            name=\"analyze_vibration_spectrum\",\n",
    "            description=\"分析振动频谱数据，输入JSON格式: {'frequencies': [频率列表], 'amplitudes': [幅值列表]}\",\n",
    "            func=analyze_vibration_spectrum\n",
    "        ),\n",
    "        Tool(\n",
    "            name=\"predict_remaining_life\",\n",
    "            description=\"预测设备剩余寿命，输入JSON格式: {'condition_score': 0.8, 'operating_hours': 8760, 'maintenance_quality': 0.8}\",\n",
    "            func=predict_remaining_life\n",
    "        ),\n",
    "        Tool(\n",
    "            name=\"generate_maintenance_plan\",\n",
    "            description=\"生成维护计划，输入JSON格式: {'issues': ['问题列表'], 'criticality': 'high/medium/low'}\",\n",
    "            func=generate_maintenance_plan\n",
    "        )\n",
    "    ]\n",
    "    \n",
    "    return tools\n",
    "\n",
    "# 演示高级工具\n",
    "print(\"🛠️ 高级PHM工具演示\")\n",
    "advanced_tools = create_advanced_phm_tools()\n",
