    "        self.monitoring = True\n",
    "        self.monitor_thread = threading.Thread(target=self._monitor_loop)\n",
    "        self.monitor_thread.daemon = True\n",
    "        self.monitor_thread.start()\n",
    "        print(\"📊 性能监控已启动\")\n",
