    "# 演示动态计算图构建\n",
    "if PROJECT_AVAILABLE:\n",
    "    print(\"🕸️ 动态计算图(DAG)演示\")\n",
    "    print(\"=\"*40)\n",
    "    \n",
    "    # 创建示例数据\n",
    "    fs = 2000\n",
    "    signal_length = 2048\n",
    "    t = np.linspace(0, signal_length/fs, signal_length)\n",
    "    \n",
    "    # 模拟轴承故障信号\n",
    "    bearing_signal = (\n",
    "        np.sin(2 * np.pi * 30 * t) +  # 30Hz 基频\n",
    "        0.3 * np.sin(2 * np.pi * 157 * t) +  # 157Hz 轴承故障频率\n",
    "        0.1 * np.random.randn(len(t))  # 噪声\n",
    "    )\n",
    "    bearing_signal = bearing_signal.reshape(1, -1, 1)\n",
    "    \n",
    "    print(f\"📊 输入信号: 形状{bearing_signal.shape}, 采样率{fs}Hz\")\n",
    "    \n",
    "    try:\n",
    "        # 创建InputData节点\n",
    "        input_data = InputData(\n",
    "            node_id=\"bearing_sensor\",\n",
    "            parents=[],\n",
    "            shape=bearing_signal.shape,\n",
    "            results={\"signal\": bearing_signal},\n",
    "            meta={\"fs\": fs, \"sensor_type\": \"accelerometer\"}\n",
    "        )\n",
    "        \n",
    "        print(f\"✅ InputData节点创建成功\")\n",
    "        print(f\"   节点ID: {input_data['node_id']}\")\n",
